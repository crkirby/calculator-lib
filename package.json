{
  "name": "calculator-lib",
  "version": "1.0.2",
  "description": "Calculate infix / postfix expressions",
  "main": "./dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require babel-core/register \"test/**/*.spec.js\"",
    "build": "NODE_ENV=production ./node_modules/.bin/babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/calculator-lib.git"
  },
  "keywords": [
    "Calculator",
    "infix",
    "postfix",
    "expression"
  ],
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/calculator-lib/issues"
  },
  "homepage": "https://github.com/felamaslen/calculator-lib#readme",
  "babel": {
    "env": {
      "production": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "4",
                "browsers": [
                  "last 2 versions",
                  "ie >= 6"
                ]
              }
            }
          ],
          "stage-0"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "4"
              }
            }
          ],
          "stage-2"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2"
  }
}
